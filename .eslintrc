{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-undef": "off",
    "no-use-before-define": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "object-curly-spacing": ["error", "never"],
    "indent": "off",
    "max-len": "off",
    "react/jsx-indent": "off",
    "react/jsx-wrap-multilines": "off"
  }
}